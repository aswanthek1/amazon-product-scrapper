{
    "name": "Product Scrapper",
    "manifest_version": 3,
    "version": "0.0.1",
    "description": "Scrape information from Products",
    "action": {
      "default_popup": "./popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["./content.js"]
      }
    ],
    "permissions": ["tabs", "storage"],
    "host_permissions": [
        "<all_urls>"
    ]
  }